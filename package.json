{
  "private": true,
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.7",
    "node-sass": "^3.7.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.0.0",
    "google-maps": "~3.2.1",
    "jquery": "^2.0",
    "laravel-elixir": "^5.0.0",
    "node-sass": "^3.8.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery.flot": "./resources/assets/js/flot/jquery.flot.js",
    "jquery.flot.pie": "./resources/assets/js/flot/jquery.flot.pie.js",
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"
  },
  "browserify-shim": {
    "jquery.flot": {
      "depends": [
        "jquery"
      ]
    },
    "jquery.flot.pie": {
      "depends": [
        "jquery.flot"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
